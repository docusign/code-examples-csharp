{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DocuSign": {
    "ClientId": "{CLIENT_ID}",
    "ClientSecret": "{CLIENT_SECRET}",
    "AuthorizationEndpoint": "https://account-d.docusign.com/oauth/auth",
    "TokenEndpoint": "https://account-d.docusign.com/oauth/token",
    "UserInformationEndpoint": "https://account-d.docusign.com/oauth/userinfo",
    "AppUrl": "http://localhost:8080",
    "SignerEmail": "{USER_EMAIL}",
    "SignerName": "{USER_FULLNAME}",
    "GatewayAccountId": "{PAYMENT_GATEWAY_ID}",
    "GatewayName": "{ACCOUNT_NAME}",
    "GatewayDisplayName": "{PAYMENT_GATEWAY}"
  },
  "AllowedHosts": "*"
}
